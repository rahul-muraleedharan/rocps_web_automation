	public boolean newStreamConfig( String path, String workBookName, String sheetName, String testCase, int occurence ) throws Exception
	{

		HashMap<String, ArrayList<String>> map = excelReader.readDataByColumn( path, workBookName, sheetName, testCase );
		streamName = map.get( "StreamName" ).get( 0 );
		partition = map.get( "Partition" ).get( 0 );
		NavigationHelper.navigateToScreen( "Streams" );
		assertEquals( NavigationHelper.getScreenTitle(), "Stream Search" );
		ButtonHelper.click( "ClearButton" );
		GenericHelper.waitForLoadmask( searchScreenWaitSec );
		boolean flag = genericHelperObj.isGridTextValuePresent( "stmName", streamName, "Name" );
		if ( !flag )
		{
			strImplObj.clickNewAction( streamName, partition );
			strImplObj.validateScreenTitle();
			TextBoxHelper.type( "Stream_Name", streamName );
		}
		else
			Log4jHelper.logInfo( "Stream '" + streamName + "' is already present with name" + streamName );
		return flag;
	}

	public boolean editStreamConfig( String path, String workBookName, String sheetName, String testCase, int occurence ) throws Exception
	{
		HashMap<String, ArrayList<String>> map = excelReader.readDataByColumn( path, workBookName, sheetName, testCase, occurence );
		streamName = map.get( "StreamName" ).get( 0 );
		partition = map.get( "Partition" ).get( 0 );
		NavigationHelper.navigateToScreen( "Streams" );
		assertEquals( NavigationHelper.getScreenTitle(), "Stream Search" );
		boolean flag = genericHelperObj.isGridTextValuePresent( "stmName", streamName, "Name" );
		if ( flag )
		{
			GridHelper.clickRow( "SearchGrid", streamName, "Name" );
			strImplObj.clickEditAction( streamName );
			assertEquals( NavigationHelper.getScreenTitle(), "Edit Stream" );
		}
		else
			Log4jHelper.logInfo( "Stream '" + streamName + "' is not available with name" + streamName );
		return flag;
	}

