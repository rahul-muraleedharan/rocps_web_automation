	/*
	 * This method is to delete Stream
	 */
	public void streamDelete( String path, String workBookName, String sheetName, String testCase, int occurence ) throws Exception
	{

		HashMap<String, ArrayList<String>> map = excelReader.readDataByColumn( path, workBookName, sheetName, testCase );
		streamName = map.get( "StreamName" ).get( 0 );
		partition = map.get( "Partition" ).get( 0 );
		NavigationHelper.navigateToScreen( "Streams" );
		genericHelperObj.selectPartitionFilter( partition, "Non Deleted Items" );
		boolean isStreamPresent = genericHelperObj.isGridTextValuePresent( "stmName", streamName, "Name" );
		if ( isStreamPresent )
		{
			strImplObj.clickDeleteAction( streamName );
			genericHelperObj.selectPartitionFilter( partition, "Deleted Items" );
			assertTrue( GridHelper.isValuePresent( "SearchGrid", streamName, "Name" ), streamName );
			Log4jHelper.logInfo( "Stream is deleted successfully : " + streamName );

		}
		else
		{
			Log4jHelper.logInfo( "Stream is not available : " + streamName );
		}
	}

	/*
	 * This method is to un delete Stream
	 */
	public void streamUnDelete( String path, String workBookName, String sheetName, String testCase, int occurence ) throws Exception
	{

		HashMap<String, ArrayList<String>> map = excelReader.readDataByColumn( path, workBookName, sheetName, testCase );
		streamName = map.get( "StreamName" ).get( 0 );
		partition = map.get( "Partition" ).get( 0 );
		NavigationHelper.navigateToScreen( "Streams" );
		genericHelperObj.selectPartitionFilter( partition, "Deleted Items" );

		boolean isStreamPresent = genericHelperObj.isGridTextValuePresent( "stmName", streamName, "Name" );
		if ( isStreamPresent )
		{
			strImplObj.clickUnDeleteAction( streamName );
			genericHelperObj.selectPartitionFilter( partition, "Non Deleted Items" );
			assertTrue( GridHelper.isValuePresent( "SearchGrid", streamName, "Name" ), streamName );
			Log4jHelper.logInfo( "Stream is un deleted successfully : " + streamName );

		}
		else
		{
			Log4jHelper.logInfo( "Stream is not available : " + streamName );
		}
	}

