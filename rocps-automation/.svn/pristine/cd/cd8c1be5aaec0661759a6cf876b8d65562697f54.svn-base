package com.subex.rocps.automation.helpers.application.roaming.roamingConfig;

import com.subex.rocps.automation.helpers.application.genericHelpers.PSActionImpl;
import com.subex.rocps.automation.helpers.selenium.PSAcceptanceTest;

public class RoamingConfigActionImpl extends PSAcceptanceTest
{
	PSActionImpl psActionImpl = new PSActionImpl();

	/* This method is used to click on 'New' action in 'Roaming Configuration'*/
	public void clickNewAction( String clientPartition ) throws Exception
	{
		psActionImpl.clickNewAction( clientPartition, "PS_Detail_roamingConfig_basicDetailTab_xpath" );

	}

	/* This method is used to click on  action in 'Roaming Configuration'*/
	public void clickOnAction( String parentActionNm, String childActionNm ) throws Exception
	{
		psActionImpl.clickOnAction( parentActionNm, childActionNm, "PS_Detail_roamingConfig_basicDetailTab_xpath" );
	}
}
